<AnchorPane fx:id="container" prefHeight="283.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ul.quickcart.controller.ResetPasswordFormController">
